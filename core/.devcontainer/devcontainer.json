{
    "name": "briefcast_core",
    "dockerComposeFile": [
        "../../docker-compose.yml",
    ],
    "service": "briefcast_core",
    "workspaceFolder": "/app",
    "customizations": {
        "vscode": {
            "extensions": [
				"denoland.vscode-deno",
				"GitHub.copilot"
			]
        }
    },
    "mounts": [
        "source=${localWorkspaceFolder}/,target=/app,type=bind,consistency=cached"
    ]
}